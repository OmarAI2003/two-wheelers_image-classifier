{
  "Comment": "A description of my state machine",
  "StartAt": "ImageDataSerielizer",
  "States": {
    "ImageDataSerielizer": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "OutputPath": "$.Payload",
      "Parameters": {
        "Payload.$": "$",
        "FunctionName": "arn:aws:lambda:us-east-1:959199095557:function:SerializeLambda:$LATEST"
      },
      "Retry": [
        {
          "ErrorEquals": [
            "Lambda.ServiceException",
            "Lambda.AWSLambdaException",
            "Lambda.SdkClientException",
            "Lambda.TooManyRequestsException"
          ],
          "IntervalSeconds": 1,
          "MaxAttempts": 3,
          "BackoffRate": 2
        }
      ],
      "Next": "ClassifyImages"
    },
    "ClassifyImages": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
